{
    "arrowParens": "avoid",
    "useTabs": true,
    "singleQuote": true,
    "tabWidth": 4,
    "semi": false,
    "trailingComma": "none",
    "importOrder": 
    [
        "<THIRD_PARTY_MODULES>",
        "^@/core/(.*)$",
        "^@/core/components(.*)$",
        "^@/components/(.*)$",
        "^@/components/layout/(.*)$",
        "^@/components/screens/(.*)$",
        "^@/components/ui/(.*)$",
        "^@/config/(.*)$",
        "^@/utils/(.*)$",
        "^@/services/(.*)$",
        "^@/store/(.*)$",
        "^@/api/(.*)$",
        "^.+\\.(template\\.html|module\\.scss$",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}